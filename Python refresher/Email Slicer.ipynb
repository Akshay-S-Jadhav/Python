   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter your email address: asj@gmail.com\n",
      "Username: asj\n",
      "Domain: gmail.com\n"
     ]
    }
   ],
   "source": [
    "def email_slicer():\n",
    "    email = input(\"Enter your email address: \").strip()\n",
    "    username = email[:email.index('@')]\n",
    "    domain = email[email.index('@') + 1:]\n",
    "    print(f\"Username: {username}\\nDomain: {domain}\")\n",
    "\n",
    "email_slicer()"
   ]
